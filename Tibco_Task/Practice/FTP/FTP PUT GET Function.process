<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="www.tibco.com/plugin/Sleep" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file">
    <pd:name>Practice/FTP/FTP PUT GET Function.process</pd:name>
    <pd:startName>File Poller</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="File Poller">
        <pd:type>com.tibco.plugin.file.FileEventSource</pd:type>
        <pd:resourceType>ae.activities.FileEventSourceResource</pd:resourceType>
        <pd:x>88</pd:x>
        <pd:y>99</pd:y>
        <config>
            <pollInterval>5</pollInterval>
            <createEvent>true</createEvent>
            <modifyEvent>true</modifyEvent>
            <deleteEvent>true</deleteEvent>
            <mode>files-and-directories</mode>
            <encoding>text</encoding>
            <sortby>File Name</sortby>
            <sortorder>descending</sortorder>
            <fileName>D:\SWAPNA\TIBCO-BW\FTP Practice\Monitor\Tibco Practice.txt</fileName>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>704</pd:endX>
    <pd:endY>99</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1470165806070</pd:targetNamespace>
    <pd:activity name="FTP Get">
        <pd:type>com.tibco.plugin.ftp.FTPGetActivity</pd:type>
        <pd:resourceType>ae.activities.FTPGetActivityUI</pd:resourceType>
        <pd:x>456</pd:x>
        <pd:y>99</pd:y>
        <config>
            <Timeout>120000</Timeout>
            <isBinary>false</isBinary>
            <Overwrite>false</Overwrite>
            <useProcessData>true</useProcessData>
            <SharedUserData>/SharedResources/FTP Connection.sharedftp</SharedUserData>
        </config>
        <pd:inputBindings>
            <FTPGetInputData>
                <RemoteFileName>
                    <xsl:value-of select="$File-Poller/ns:EventSourceOuputTextClass/fileInfo/fileName"/>
                </RemoteFileName>
            </FTPGetInputData>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="FTP Put">
        <pd:type>com.tibco.plugin.ftp.FTPPutActivity</pd:type>
        <pd:resourceType>ae.activities.FTPPutActivityUI</pd:resourceType>
        <pd:x>216</pd:x>
        <pd:y>99</pd:y>
        <config>
            <Timeout>120000</Timeout>
            <FireWall>false</FireWall>
            <isBinary>false</isBinary>
            <Append>false</Append>
            <useProcessData>true</useProcessData>
            <Overwrite>true</Overwrite>
            <SharedUserData>/SharedResources/FTP Connection.sharedftp</SharedUserData>
        </config>
        <pd:inputBindings>
            <FTPPutInputDataASCII>
                <RemoteFileName>
                    <xsl:value-of select="$File-Poller/ns:EventSourceOuputTextClass/fileInfo/fileName"/>
                </RemoteFileName>
                <Data>
                    <xsl:value-of select="$File-Poller/ns:EventSourceOuputTextClass/fileContent"/>
                </Data>
            </FTPPutInputDataASCII>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Sleep">
        <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
        <pd:resourceType>ae.activities.sleep</pd:resourceType>
        <pd:x>340</pd:x>
        <pd:y>99</pd:y>
        <config/>
        <pd:inputBindings>
            <ns2:SleepInputSchema>
                <IntervalInMillisec>
                    <xsl:value-of select="1000"/>
                </IntervalInMillisec>
            </ns2:SleepInputSchema>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Write File">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>581</pd:x>
        <pd:y>99</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
        </config>
        <pd:inputBindings>
            <ns:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="'D:\SWAPNA\TIBCO-BW\FTP Practice\Create Dir\Tibco BW.txt'"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="$FTP-Get/FTPGetOutputASCII/ASCIIData"/>
                </textContent>
            </ns:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>File Poller</pd:from>
        <pd:to>FTP Put</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FTP Put</pd:from>
        <pd:to>Sleep</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Sleep</pd:from>
        <pd:to>FTP Get</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FTP Get</pd:from>
        <pd:to>Write File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Write File</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>