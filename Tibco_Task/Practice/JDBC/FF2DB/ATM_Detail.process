<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
    <pd:name>Practice/JDBC/FF2DB/ATM_Detail.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>134</pd:startX>
    <pd:startY>88</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>489</pd:endX>
    <pd:endY>98</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1470475878371</pd:targetNamespace>
    <pd:activity name="Parse Data">
        <pd:type>com.tibco.plugin.parse.ParseActivity</pd:type>
        <pd:resourceType>ae.activities.ParseActivity</pd:resourceType>
        <pd:x>253</pd:x>
        <pd:y>94</pd:y>
        <config>
            <InputType>File</InputType>
            <Encoding>ASCII</Encoding>
            <ParseSharedConfig>/SharedResources/XML/ATM_Info/Data Format.sharedparse</ParseSharedConfig>
        </config>
        <pd:inputBindings>
            <Input>
                <fileName>
                    <xsl:value-of select="'D:\SWAPNA\MY Work\XML Prcatice\ATMDetail.txt'"/>
                </fileName>
                <noOfRecords>
                    <xsl:value-of select="-1"/>
                </noOfRecords>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="JDBC Update">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>366</pd:x>
        <pd:y>100</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>insert into ATM  values (?,?,?,?,?,?)</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>CardNo</parameterName>
                    <dataType>BIGINT</dataType>
                </parameter>
                <parameter>
                    <parameterName>ValidDate</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>ExpDate</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>Name</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>CVV</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>Pin</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <batchUpdate>true</batchUpdate>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <xsl:for-each select="$Parse-Data/Output/Rows/ATM_Card">
                    <Record>
                        <CardNo>
                            <xsl:value-of select="CardNo"/>
                        </CardNo>
                        <ValidDate>
                            <xsl:value-of select="ValidDate"/>
                        </ValidDate>
                        <ExpDate>
                            <xsl:value-of select="ExpDate"/>
                        </ExpDate>
                        <Name>
                            <xsl:value-of select="Name"/>
                        </Name>
                        <CVV>
                            <xsl:value-of select="CVV"/>
                        </CVV>
                        <Pin>
                            <xsl:value-of select="Pin"/>
                        </Pin>
                    </Record>
                </xsl:for-each>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Parse Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse Data</pd:from>
        <pd:to>JDBC Update</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Update</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>