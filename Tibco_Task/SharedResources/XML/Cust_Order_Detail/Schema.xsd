<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.tibco.com/schemas/Tibco_Task/SharedResources/XML/Cust_Order_Detail/Schema.xsd"
	 targetNamespace="http://www.tibco.com/schemas/Tibco_Task/SharedResources/XML/Cust_Order_Detail/Schema.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="Cust_OrderDetail">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Order" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Cust_OrderNo" type="xs:int"/>
							<xs:element name="Cust_Name" type="xs:string"/>
							<xs:element name="Cust_Address" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>