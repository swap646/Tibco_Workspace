<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.tibco.com/schemas/Tibco_Task/SharedResources/XML/PatientDetail/Schema.xsd"
	 targetNamespace="http://www.tibco.com/schemas/Tibco_Task/SharedResources/XML/PatientDetail/Schema.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="PatientDetail">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Patient" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="patientname" type="xs:string"/>
							<xs:element name="patientno" type="xs:int"/>
							<xs:element name="doctorname" type="xs:string"/>
							<xs:element name="patientroomno" type="xs:int"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>